{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": [
    "google",
    "prettier",
    "plugin:prettier/react",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": "error"
  }
}
